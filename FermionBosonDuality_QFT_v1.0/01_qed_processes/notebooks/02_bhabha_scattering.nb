Cell[BoxData[
 InterpretationBox[
  RowBox[{"\<\"Center-of-mass scattering cross section (conventional \
calculation)\:ff1b\"\>", "\[InvisibleSpace]", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"512", " ", 
      SuperscriptBox["pu", "4"], " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["m", "2"], "+", 
         SuperscriptBox["pu", "2"]}], ")"}], "3"]}]], 
    RowBox[{"do", " ", 
     SuperscriptBox["e1", "4"], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"32", " ", 
        SuperscriptBox["m", "8"]}], "+", 
       RowBox[{"80", " ", 
        SuperscriptBox["m", "6"], " ", 
        SuperscriptBox["pu", "2"]}], "+", 
       RowBox[{"204", " ", 
        SuperscriptBox["m", "4"], " ", 
        SuperscriptBox["pu", "4"]}], "+", 
       RowBox[{"248", " ", 
        SuperscriptBox["m", "2"], " ", 
        SuperscriptBox["pu", "6"]}], "+", 
       RowBox[{"99", " ", 
        SuperscriptBox["pu", "8"]}], "+", 
       RowBox[{"4", " ", 
        SuperscriptBox["m", "2"], " ", 
        SuperscriptBox["pu", "2"], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"28", " ", 
           SuperscriptBox["m", "4"]}], "+", 
          RowBox[{"40", " ", 
           SuperscriptBox["m", "2"], " ", 
           SuperscriptBox["pu", "2"]}], "+", 
          RowBox[{"15", " ", 
           SuperscriptBox["pu", "4"]}]}], ")"}], " ", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "+", 
       RowBox[{"4", " ", 
        SuperscriptBox["pu", "4"], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"13", " ", 
           SuperscriptBox["m", "4"]}], "+", 
          RowBox[{"18", " ", 
           SuperscriptBox["m", "2"], " ", 
           SuperscriptBox["pu", "2"]}], "+", 
          RowBox[{"7", " ", 
           SuperscriptBox["pu", "4"]}]}], ")"}], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"2", " ", "\[Theta]"}], "]"}]}], "+", 
       RowBox[{"4", " ", 
        SuperscriptBox["m", "2"], " ", 
        SuperscriptBox["pu", "6"], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"3", " ", "\[Theta]"}], "]"}]}], "+", 
       RowBox[{
        SuperscriptBox["pu", "8"], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"4", " ", "\[Theta]"}], "]"}]}]}], ")"}], " ", 
     SuperscriptBox[
      RowBox[{"Csc", "[", 
       FractionBox["\[Theta]", "2"], "]"}], "4"]}]}]}],
